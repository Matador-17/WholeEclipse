public class PageInitializer {
	
	public static DashboardPageElements dash;
	public static AddEmployeePageElements addEmployee;
	public static LeaveListPageElements leave;
	public static PersonalDetailsPageElements pdetails;
	public static LoginPageElements login;
	
	public static void initializeAllPages() {
		
		LoginPageElements login=new LoginPageElements();
		DashboardPageElements dash=new DashboardPageElements();
		AddEmployeePageElements addEmployee=new AddEmployeePageElements();
		LeaveListPageElements leave=new LeaveListPageElements();
		PersonalDetailsPageElements pdetails=new PersonalDetailsPageElements();
		
	}
	
	
	
}
